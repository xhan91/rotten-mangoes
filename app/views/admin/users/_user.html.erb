<tr>
  <td><%= user.id %></td>
  <td><%= user.full_name %><%= " | Admin" if user.admin %></td>
  <td><%= user.email %></td>
  <td><%= user.reviews.count %></td>
  <td>
    <%= link_to "Detail", admin_user_path(user) %> |
    <%= link_to "Edit", edit_admin_user_path(user) %> | 
    <%= link_to "Delete", admin_user_path(user), method: :delete, data: {confirm: "You sure?"} %> |
    <%= link_to "Fake", session_fake_path(user), method: :post %>
  </td>
</tr>